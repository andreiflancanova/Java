package Lista0905_Lacos_Repeticao;

public class Q01_For_Lista_09_05 
{
/*	Informar todos os números de 1000 a 1999 que,
 * 	quando divididos por 11, obtemos resto = 5.
 
 */
	public static void main(String[] args) 
	{
		// Declaração de Variáveis
		int N,C=0;
		
		//Loop For de 1000 até 1999
		for (N=1000;N<=1999;N++)
		{
			//Estrutura para ele quebrar de linha de 10 em 10
			//Utilizando if
			if (N%11==5)
			{
				C++;
				if (C%10==0)
				{
					System.out.print("\n "+N+", ");
				}
				else
				{
					System.out.print(+N+", ");
				}
			}
			else if(N==1999)
			{
				System.out.print(+N+". ");
			}
			else
			{
			
			}
		}
		
	}

}
